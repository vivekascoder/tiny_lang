// Function to return greatest among three passed numbers.
fun greatest(a: usize, b: usize, c: usize) => usize {
    if (a > b) {
        if (a > c) {
            return a;
        } else {
            return c;
        }
    } else {
        if (b > c) {
            return b;
        } else {
            return c;
        }
    }
}


